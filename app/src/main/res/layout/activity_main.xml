<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:background="@color/black">



<!--Heart parent layout-->
    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="140dp"
        android:layout_height="40dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="@dimen/heart_margin"
        android:layout_marginStart="@dimen/heart_margin">

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/heart1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            app:srcCompat="@drawable/heart" />

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/heart2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_marginHorizontal="10dp"
            app:srcCompat="@drawable/heart" />

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/heart3"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            app:srcCompat="@drawable/heart" />
    </androidx.appcompat.widget.LinearLayoutCompat>

    <!--score-->
    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/score"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="@dimen/score_margin"
        android:layout_marginEnd="@dimen/score_margin"
        android:textColor="@color/white"
        android:textSize="@dimen/score_text_size"
        android:textStyle="bold"
        tools:text="000" />

    <androidx.appcompat.widget.LinearLayoutCompat
        android:id="@+id/startMenuScreen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:orientation="vertical"
        android:layout_centerInParent="true"
        >

        <!--Sensors Switch-->
        <Switch
            android:id="@+id/sensors_switch"
            android:layout_width="320dp"
            android:layout_height="wrap_content"
            android:textColor="@color/yellow"
            android:textSize="@dimen/score_text_size"
            android:textStyle="bold"
            android:text="Sensors mode!"
            android:layout_gravity="center"
            android:padding="@dimen/startMenuPadding"
            tools:ignore="UseSwitchCompatOrMaterialXml"
            android:checked="false"
            android:clickable="true"
            android:background="#9C786C"/>

        <!--Speed Switch-->
        <Switch
            android:id="@+id/speed_switch"
            android:layout_width="320dp"
            android:layout_height="wrap_content"
            android:textColor="@color/yellow"
            android:textSize="@dimen/score_text_size"
            android:textStyle="bold"
            android:text="Speed mode!"
            android:layout_gravity="center"
            android:padding="@dimen/startMenuPadding"
            tools:ignore="UseSwitchCompatOrMaterialXml"
            android:checked="false"
            android:clickable="true"
            android:background="#9C786C"
            android:layout_marginBottom="15dp"/>

        <com.google.android.material.floatingactionbutton.ExtendedFloatingActionButton
            android:id="@+id/GamePlayButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:clickable="true"
            android:text="Play!"
            android:textStyle="bold"
            android:textSize="30sp"
            android:backgroundTint="@color/yellow"
            android:padding="@dimen/startMenuPadding"/>

    </androidx.appcompat.widget.LinearLayoutCompat>

